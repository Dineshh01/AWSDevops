[QASERVERS]
QAServer01 ansible_host=IP ansible_ssh_user=your_ssh_user ansible_become=yes ansible_become_user=root ansible_become_pass=your_root_password
QAServer02 ansible_host=IP ansible_ssh_user=your_ssh_user ansible_become=yes ansible_become_user=root ansible_become_pass=your_root_password

[dev_servers]
devserver1 ansible_host=IP ansible_ssh_user=your_ssh_user ansible_become=yes ansible_become_user=root ansible_become_pass=your_root_password
devserver2 ansible_host=IP ansible_ssh_user=your_ssh_user ansible_become=yes ansible_become_user=root ansible_become_pass=your_root_password

